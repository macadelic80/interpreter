    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch tokenizer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app/main.js",
            "args": [
                "tokenize",
                "test.lox"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch parse",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app/main.js",
            "args": [
                "parse",
                "test.lox"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch evaluate",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app/main.js",
            "args": [
                "evaluate",
                "test.lox"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch run",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app/main.js",
            "args": [
                "run",
                "test.lox"
            ]
        }
    ]
}